{
  "version": 1,
  "paths": {
    "/bookings": {
      "name": "/bookings",
      "lambdaFunction": "bookingsHandler",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/profiles": {
      "name": "/profiles",
      "lambdaFunction": "profilesHandler",
      "permissions": {
        "setting": "open"
      }
    }
  }
}